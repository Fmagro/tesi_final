<h1> Log in </h1>
<%= form_for :session, url: login_path do |f| %>
 
  <div class="infosection">
  <p>
    <%= f.label :email, "Email:" %><br>
    <%= f.text_field :email %>
  </p>
 
   <p>
    <%= f.label :password, "Password:" %><br>
    <%= f.password_field :password%><br>
   </p>
 
   <p>
    <%= f.submit "Log in" %>
  </p>
    <p> <%= link_to "Register?", new_user_path %></p>
  </div>
<% end %>

