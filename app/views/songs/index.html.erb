<h1>Listing Songs</h1>
<%= link_to 'New song', new_song_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Genre</th>
    <th colspan="4"></th>
  </tr>
 
  <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.genre %></td>
      <td><%= link_to 'Show', song_path(song) %></td>
      <td><%= link_to 'Edit', edit_song_path(song) %></td>
      <td><%= link_to 'Manage Link', managelink_song_path(song) %></td>
      <td><%= link_to 'Destroy', song_path(song),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Search for songs', songsearch_songs_path %><br>

